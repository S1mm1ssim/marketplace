<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="2022-12-08-add-unique-user-username-key" author="Andrdemyan4ik@gmail.com">
        <addUniqueConstraint tableName="user" columnNames="username" constraintName="user_username_key"/>
        <rollback>
            <dropUniqueConstraint tableName="user" constraintName="user_username_key"/>
        </rollback>
    </changeSet>

</databaseChangeLog>
